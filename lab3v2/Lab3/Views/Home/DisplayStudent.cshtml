@model Lab3.Models.Student

@{
    ViewData["Title"] = "Display Student";
}


<h3>Display Form Data</h3>
<table>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.FirstName
        </td>
    </tr>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.LastName
        </td>
    </tr>
    <tr>
        <td>
            Student Id:
        </td>
        <td>
            @Model.StudentId
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.EmailAddress
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.Desc
        </td>
    </tr>
</table>
